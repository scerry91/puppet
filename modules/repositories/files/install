debianver=`lsb_release -cs`
echo "deb http://archive.debian.org/debian $debianver main contrib non-free" >> /etc/apt/source.list
echo "deb http://security.debian.org/ $debianver/updates main" >> /etc/apt/source.list
echo "deb http://security.debian.org/ $debianver/updates main" >> /etc/apt/source.list
echo "deb http://ftp.uk.debian.org/debian/ $debianver-updates main" >> /etc/apt/source.list
apt-get update
apt-get upgrade -y

