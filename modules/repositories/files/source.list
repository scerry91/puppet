debianver=$(lsb_release -cs)
os=$(lsb_release -is)
echo "deb http://archive.debian.org/$os/ $debianver main contrib non-free" >> /etc/apt/source.list
echo "deb http://security.debian.org/$os/ $debianver/updates main" >> /etc/apt/source.list
echo "deb http://ftp.uk.debian.org/$os/ $debianver main" >> /etc/apt/source.list
echo "deb http://ftp.uk.debian.org/$os/ $debianver-updates main" >> /etc/apt/source.list
sudo apt-get update
