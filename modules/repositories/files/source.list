debianver=`lsb_release -cs`

<% if location =="jessie" -%>
deb http://archive.debian.org/debian <%= debianver %> main contrib non-free
deb http://security.debian.org/ <%= debianver %>/updates main
<%elseif debianver == "wheezy" -%
deb http://security.debian.org/ <%= debianver %>/updates main
deb http://ftp.uk.debian.org/debian/ <%= debianver %>-updates main
<% end -%>

